<div class="title">Blog Posts Dashboard</div>
{{#each posts}}
<div class="wrapper">
  <a href="/post/{{id}}">
    <div class="card">
      <div class="card-header">
        {{post_title}}
      </div>
      <div class="card-body">
        <blockquote class="blockquote mb-0">
          <p>{{post}}</p>
          <footer class="blockquote-footer">{{user.user_name}}</footer>
        </blockquote>
      </div>
    </div>
</div>
{{/each}}

